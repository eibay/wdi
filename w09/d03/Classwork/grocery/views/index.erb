	<script src='./app.js'></script>
	<link rel="stylesheet" href="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/themes/smoothness/jquery-ui.css" />
	<script src="//ajax.googleapis.com/ajax/libs/jqueryui/1.11.1/jquery-ui.min.js"></script>
	<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
	<style>
	li{
		cursor: pointer;
	}
	.addquantity{
		cursor: pointer;
	}
	.removequantity{
		cursor: pointer;
	}
	.quantity{
		display:inline;
	}
	.lists{
		min-height: 100px;
		width:30%;

	}
	.trash{
		float:right;
	}
	.container{
		width:50%;
	}
	.trash{
		font-size:200px;
		margin-top:150px;
		height:300px;
		width:200px;
	}
	.quantity{
		font-size:20px;
		font-weight: bold
	}
	li{
		font-size:15px;
	}
	.quantity{
		padding-left:10px;
	}
	.fa-stack{
		font-size:200px;
	}
	.happy{
		font-size:210px;
	}
	.happy{
		display:inline;
	}

	</style>
	<script type="text/template" id="need-template">
	<%=item.item%><div class="quantity"><%=item.quantity%></div><label class="addquantity"><i class="fa fa-arrow-up"></i></label><label class="removequantity"><i class="fa fa-arrow-down"></i></label>
	</script>
	<script type="text/template" id="bought-template">
	<%=item.item%><div class="quantity"></div><label class="addquantity"></label><label class="removequantity"></label>
	</script>
</head>
<body>
	

	<span class="fa-stack fa-lg">
		<i class="fa fa-wheelchair fa-stack-1x"></i>
		<i class="fa fa-bullseye fa-stack-2x text-danger" style="color:red;"></i>



	</span>
	<label class="happy">

		<i class="fa fa-truck"></i>
		=
		<i class="fa fa-smile-o"></i>
	</label>

	<div class="container">





		<div class="trash lists"><i class="fa fa-trash"></i></div>

		<h1>Grocery List</h1>
		<input type="text" id="additem" placeholder="Add An Item"><br>
		<input type="text"id="quantity" placeholder="Quantity"><br>
		<button id="addIt">Add It</button>
		<h2>Need</h2>
		<ul class="list lists">

		</ul>
