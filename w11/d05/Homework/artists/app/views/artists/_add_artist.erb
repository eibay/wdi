	<h3>Add an Artist</h3>
	<form action="/artists" method="POST">
		<label>
			Artist Name:
			<input name="name" />
		</label>
		<label>
			Genre:
			<input name="genre" />
		</label>
		<label>
			Number of Albums:
			<input name="number_of_albums" />
		</label>
		<label>
			Photo:
			<input name="photo_url" />
		</label>

	  <input name="authenticity_token" value="<%= form_authenticity_token() %>" type="hidden">

	  <button>Submit</button>
	</form>
