<%= form_for :puppy, url: puppies_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
 
  <p>
    <%= f.label :breed %><br>
    <%= f.text_field :breed %>
  </p>
 
  <p>
    <%= f.label :cuteness %><br>
    <%= f.number_field :cuteness %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>