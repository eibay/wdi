<html>
  <head>
    <title>List of Dog Breed</title>
  </head>
  <body>
    <div id="wrapper"
    <form action="/" method="POST">
      <input type="text" name="breed" placeholder="Dog Breed">
      <button>Submit</button>
      <hr />
      <h1>Dog Breeds</h1>
      <ul>
        <% breeds.each do |breed| %>
        <li><%= breed.name %><form action="/<%= breed.id %>" method="POST"><input type="hidden" name="_method" value="PUT"><Button>Edit</Button></li>
        <% end %>
      </ul>
    </div>
  </body>
</html>
