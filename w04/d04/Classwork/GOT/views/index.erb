<html>
<head>
	<title>House and Characters</title>
</head>
<body>

	<h1>Houses and Characters</h1>

<a href='./characters'>Add New Character</a>
<h3> All Houses</h3>
	<ul>
		<% houses.each do |house| %>
		<li><a href='/houses/<%= house["id"] %>'> <%= house["name"] %></a></li>
		<% end %>
	</ul>
<h3> All Characters</h3>

<ul>
	<% characters.each do |character| %>
	<li><a href="/characters/<%= character['id'] %>"><%= character["first"] %> <%= character["last"] %></a></li>
	<% end %>
</ul>



</body>
</html>